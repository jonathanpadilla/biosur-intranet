                          <div class="checkbox-custom mb10">
                            <input type="checkbox" name="detalle[10][dia][3]" id="detalle[10][dia3]">
                            <label for="detalle[10][dia3]">Miercoles</label>
                          </div>
                          <div class="checkbox-custom mb10">
                            <input type="checkbox" name="detalle[10][dia][4]" id="detalle[10][dia4]">
                            <label for="detalle[10][dia4]">Jueves</label>
                          </div>
                          <div class="checkbox-custom mb10">
                            <input type="checkbox" name="detalle[10][dia][5]" id="detalle[10][dia5]">
                            <label for="detalle[10][dia5]">Viernes</label>
                          </div>
                          <div class="checkbox-custom mb10">
                            <input type="checkbox" name="detalle[10][dia][6]" id="detalle[10][dia6]">
                            <label for="detalle[10][dia6]">Sabado</label>
                          </div>
                          <div class="checkbox-custom mb10">
                            <input type="checkbox" name="detalle[10][dia][7]" id="detalle[10][dia7]">
                            <label for="detalle[10][dia7]">Domingo</label>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="row">
                      <div class="col-md-12">
                        <h3>Información domiciliaria</h3>
                      </div>
                    </div>

                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group">
                          <label for="select_comuna" class="control-label">Comuna*</label>
                          <select name="detalle[10][comuna]" id="select_comuna_10" class="form-control select2-single">
                              {% for comuna in comunas %}
                                <option value="{{ comuna.id }}">{{ comuna.nombre }}</option>
                              {% endfor %}
                          </select>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">

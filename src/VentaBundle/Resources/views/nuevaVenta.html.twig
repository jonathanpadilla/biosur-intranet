                        <dd id="info_tipo_pago"></dd>
                        <dt>Comentario</dt>
                        <dd id="info_comentario"></dd>
                      </dl>
                    </div>

                    <table class="table table-bordered mbn">
                      <thead>
                        <tr>
                          <th>Servicio</th>

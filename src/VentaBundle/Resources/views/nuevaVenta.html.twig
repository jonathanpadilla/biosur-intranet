                        </tr>
                      </thead>
                      <tbody id="table_listadetalle">
                      </tbody>
                      <tfoot>

                        <tr>
                          <td colspan="4"></td>
                          <td id="into_totaldetalle"></td>
                        </tr>

                      </tfoot>
                    </table>

                  </div>
                </div>
              <!-- end section -->
            </section>
          </div>
          <!-- End: Wizard -->

        </form>
        <!-- End Account2 Form -->

      </div>


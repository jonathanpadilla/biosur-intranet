                      <dd id="dd_regioncliente"></dd>
                      <dt>Provincia</dt>
                      <dd id="dd_provinciacliente"></dd>
                      <dt>Comuna</dt>
                      <dd id="dd_comunacliente"></dd>
                      <dt>Dirección</dt>
                      <dd id="dd_direccioncliente"></dd>
                    </dl>
                  </div>
                  <div class="col-md-12">

                    <h3>Información de contrato</h3>

                    <div class="col-md-6">

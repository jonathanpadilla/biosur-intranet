                            <label for="detalle[10][dia1]">Lunes</label>
                          </div>
                          <div class="checkbox-custom mb10">
                            <input type="checkbox" name="detalle[10][dia][2]" id="detalle[10][dia2]">
                            <label for="detalle[10][dia2]">Martes</label>

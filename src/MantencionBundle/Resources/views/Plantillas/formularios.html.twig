<form id="form_imprimir" action="{{ url('mantencion_pdf_mantencion_diaria') }}" method="post" target="_blank">
	{% for formulario in listaFormularios %}
	<div class="row" style="border:2px solid #666;border-top: 15px solid #ea661c;">

		<div class="col-md-12 check_formularios">

			<div class="form-group">
				<div class="radio-custom radio-default mb5">
			        <input type="checkbox" id="form[{{ formulario.id }}][check_imprimir]" name="form[{{ formulario.id }}][check_imprimir]" checked>
			        <label for="form[{{ formulario.id }}][check_imprimir]">INFORMACIÓN GENERADOR</label>
					<!-- <h3>INFORMACIÓN GENERADOR</h3> -->
		        </div>
		    </div>

		</div>

		<div class="col-md-6">

			<div class="row">
				<div class="col-md-12">
					<span>Nombre Empresa / Institución / Particular</span>
					<div class="form-group">
					    <!-- <label for="input_nombre" class="control-label">Nombre*</label> -->
					    <input type="text" name="form[{{ formulario.id }}][input_nombre]" class="form-control" placeholder="" value="{{ formulario.empresaNombre }}">
					</div>
				</div>
			</div>

			<div class="row">
				<div class="col-md-12">
					<table class="table" style="border:1px solid #eeeeee;">

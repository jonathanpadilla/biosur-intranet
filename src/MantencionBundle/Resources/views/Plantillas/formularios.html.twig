							<tr>
								<th>Servicios</th>
								<th>Cantidad</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>Mantención P.T.A.S.</td>
								<td><input type="number" name="form[{{ formulario.id }}][servicio1]" class="form-control" value="{{ formulario.servicio1 }}"></td>
							</tr>
							<tr>
								<td>Mantención de Fosa</td>
								<td><input type="number" name="form[{{ formulario.id }}][servicio2]" class="form-control" value="{{ formulario.servicio2 }}"></td>
							</tr>
							<tr>
								<td>Mantención de Fosa Elevadora</td>
								<td><input type="number" name="form[{{ formulario.id }}][servicio3]" class="form-control" value="{{ formulario.servicio3 }}"></td>

								<td><input type="number" name="form[{{ formulario.id }}][servicio6]" class="form-control" value="{{ formulario.servicio6 }}"></td>
							</tr>
							<tr>
								<td>Arriendo de Baño Quimico</td>
								<td><input type="number" name="form[{{ formulario.id }}][servicio7]" class="form-control" value="{{ formulario.servicio7 }}"></td>
